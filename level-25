#include<iostream>
using namespace std;
int digit(int n)
{
    int count=0;
    while(n)
    {
        count++;
        n=n/10;
    }
    return count;
}
int main()
{
    int n,i,k,cnt=0;
    cin>>n;
    cin>>k;
    int arr[n];
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for(i=0;i<n;i++)
    {
        int r=digit(arr[i]);
        if(k==r)
        cnt++;
    }
    cout<<cnt;
}
